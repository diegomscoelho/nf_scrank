process {   
  
 withName: DOWNSAMPLE {  
        memory = { 64.GB }
        publishDir = [
            path: { "${params.outdir}/scrank/" },
            mode: 'copy'
        ]
    }
   
 withName: SCRANK {
	memory = { 80.GB }
        cpus = 64
        publishDir = [
            path: { "${params.outdir}/scrank/" },
            mode: 'copy'
        ]
    }


 withName: MERGE {
        memory = { 64.GB }
        cpus = 16
        publishDir = [
            path: { "${params.outdir}/rank_scores/" },
            mode: 'copy'
        ]
    }
}
