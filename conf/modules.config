process {   
   
 withName: SCRANK {
        errorStrategy =  { 'ignore' }
		memory = { 64.GB }
        cpus = 16
        publishDir = [
            path: { "${params.outdir}/scrank/" },
            mode: 'copy'
        ]
    }


 withName: MERGE {
        memory = { 64.GB }
        cpus = 16
        publishDir = [
            path: { "${params.outdir}/rank_scores/" },
            mode: 'copy'
        ]
    }
}
